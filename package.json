{
  "name": "detector-lancamentos-duplicados",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend-next && npm run dev\"",
    "build": "cd frontend-next && npm run build",
    "start": "cd backend && npm run dev",
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend-next && npm install"
  },
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
